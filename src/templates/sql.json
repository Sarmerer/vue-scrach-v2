{
  "sql:insert": {
    "lines": ["INSERT", "${statement.rows}", "INTO ${table}"]
  },

  "sql:update": {
    "lines": [
      "UPDATE",
      "${statement.rows}",
      "WITH",
      "${statement.with}",
      "IN ${table}"
    ]
  },

  "sql:row": {
    "lines": ["${row}"]
  },

  "sql:value": {
    "lines": ["${value}"]
  },

  "sql:row-value": {
    "lines": ["${row}: ${value}"]
  }
}
